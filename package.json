{
  "name": "react-rerouter",
  "version": "0.1.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "precommit": "lint-staged",
