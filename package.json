  "lint-staged": {
    "*.+(ts|tsx)": [
      "tslint"
    ]
  },
  "devDependencies": {
    "@types/history": "^4.5.2",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jest": "^19.2.4",
    "@types/node": "^7.0.29",
    "@types/path-to-regexp": "^1.7.0",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.42",
    "@types/react-test-renderer": "^15.5.0",
    "@types/redux-logger": "^3.0.0",
    "coveralls": "^2.13.1",
    "es6-object-assign": "^1.1.0",
    "es6-promise": "^4.1.0",
    "history": "^4.6.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "lint-staged": "^3.6.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "ts-jest": "^20.0.6",
    "ts-loader": "^2.1.0",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4",
    "typestyle": "^1.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "@types/prop-types": "^15.0.0",
    "@types/react": "^15.0.0",
    "path-to-regexp": "^1.7.0",
    "prop-types": "^15.0.0",
    "react": "^15.0.0",
    "tslib": "^1.7.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testRegex": "\\.spec\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
